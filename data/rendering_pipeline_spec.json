{
  "name": "MultiViewRenderingPipeline",
  "framework": "Blender_or_PyOpenGL",
  "viewpoints": {
    "count": 12,
    "distribution": "spherical_uniform",
    "radius": 2.0,
    "height_range": [
      -0.3,
      0.3
    ]
  },
  "output": {
    "image_format": "PNG",
    "resolution": [
      256,
      256
    ],
    "color_space": "sRGB"
  },
  "camera": {
    "fov": 45,
    "near_plane": 0.1,
    "far_plane": 100.0
  },
  "lighting": {
    "type": "three_point",
    "ambient": 0.3,
    "directional": [
      0.7,
      0.5,
      0.8
    ]
  },
  "pipeline_stages": [
    "load_model",
    "normalize_scale",
    "position_camera",
    "render_image",
    "save_output"
  ]
}