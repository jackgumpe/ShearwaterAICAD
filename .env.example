# API Keys - DO NOT COMMIT THIS FILE WITH REAL KEYS
# Copy this file to .env and fill in your actual API keys
# .env is already in .gitignore for safety

# Anthropic API Key (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key (Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# Database URL (optional)
DATABASE_URL=sqlite:///./shearwater_aicad.db

# ZMQ Configuration (optional)
ZMQ_BROKER_HOST=localhost
ZMQ_BROKER_PUB_PORT=5555
ZMQ_BROKER_SUB_PORT=5556
ZMQ_PERSISTENCE_PORT=5557
